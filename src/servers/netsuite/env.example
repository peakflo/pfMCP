# NetSuite MCP Server Environment Configuration
# Copy this file to .env and fill in your actual values

# Your NetSuite account ID (found in the URL when you log into NetSuite)
NETSUITE_ACCOUNT_ID=1234567

# OAuth 1.0a Consumer Credentials (from your NetSuite integration)
NETSUITE_CONSUMER_KEY=your_consumer_key_here
NETSUITE_CONSUMER_SECRET=your_consumer_secret_here

# OAuth 1.0a Token Credentials (from your NetSuite access token)
NETSUITE_TOKEN_ID=your_token_id_here
NETSUITE_TOKEN_SECRET=your_token_secret_here

# Optional: Set log level for debugging
LOG_LEVEL=INFO
